/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=out0);
    Nand(a=b, b=b, out=out1);
    Nand(a=out0, b=b, out=out2);
    Nand(a=out1, b=a, out=out3);
    Nand(a=out2, b=out3, out=out);
}